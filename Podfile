# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'Didu' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Didu
  pod "MarkdownView"
  pod 'SnapKit'
  pod 'DeviceKit'
  pod 'ProgressHUD'
  pod 'Toast-Swift'
  # QMUI查看与修改 iOS App 里的 UI 对象
  pod 'LookinServer', :configurations => ['Debug']
  
end
